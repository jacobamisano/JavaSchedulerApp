<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.StaffScheduleFX.PrimaryController">
   <children>
      <Label text="Please select an option from the choices below." />
      <Button fx:id="btnAddEmployee" text="Enter Employee Data" onAction="#onBtnAddEmployeeClick"/>
      <Button fx:id="btnAddTimeOff" text="Enter Time-OFf Request" onAction="#onBtnAddTimeOffClick"/>
      <Button fx:id="btnViewEmployees" text="ViewCurrentEmployees" onAction="#onBtnViewEmployeeClick"/>
      <Button fx:id="btnEditSchedulePreferences" text="ViewCurrentEmployees" onAction="#onBtnEditSchedulePreferencesClick"/>
      <Button fx:id="btnBuildSchedule" text="Build Schedule for the current week" onAction="#onBtnBuildScheduleClick"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
